alias aliases="vim ~/.bash_aliases"
alias setalias="source ~/.bash_aliases"
alias vimrc="vim ~/.vimrc"
alias vimcfg="cd ~/.vim"
alias gst="git status"
alias gaa="git add ."
alias gc="git commit"
alias gp="git push"
alias gpsu="git push --set-upstream origin main"
alias ga="git add"
alias ginit="gaa && gc -m \"initial commit\" && gp"
alias glog="git log"
alias docs="cd ~/Documents/"
alias update="sudo apt update"
alias upgrade="sudo apt upgrade"
alias install="sudo apt install"
alias sasw="sass --watch style.scss style.css"
